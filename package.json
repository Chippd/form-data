{
  "name": "formdata-node",
  "version": "2.2.2",
  "description": "FormData implementation for Node.js. Built over Readable stream and async generators.",
  "repository": "octet-stream/form-data",
  "keywords": [
    "form-data",
    "node",
    "async-iterator",
    "stream",
    "form",
    "upload",
    "files-upload"
  ],
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "main": "lib/FormData",
  "types": "type/FormData.d.ts",
  "engines": {
    "node": ">= 10.17"
  },
  "scripts": {
    "eslint": "eslint src/**/*.mjs",
    "staged": "lint-staged",
    "cleanup": "rimraf lib test polyfill.mjs polyfill.js *.map",
    "test": "ava",
    "test:types": "tsd",
    "coverage": "npm run cleanup && nyc npm run test",
    "report": "npm run cleanup && nyc npm run test && nyc report -r=html",
    "ci": "nyc npm run test && nyc report -r=json"
  },
  "nyc": {
    "extension": [
      ".mjs"
    ],
    "exclude": [
      "test/**",
      ".babelrc.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/plugin-transform-modules-commonjs": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@octetstream/eslint-config": "4.0.0",
    "@types/node": "14.0.11",
    "ava": "3.12.1",
    "babel-plugin-add-module-exports": "1.0.2",
    "codecov": "3.7.2",
    "eslint": "7.7.0",
    "eslint-plugin-ava": "10.3.0",
    "fetch-blob": "1.0.6",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "lodash.isplainobject": "4.0.6",
    "nyc": "15.1.0",
    "object-deep-from-entries": "^0.3.0",
    "proxyquire": "2.1.3",
    "rimraf": "3.0.2",
    "sinon": "9.0.3",
    "supertest": "4.0.2",
    "then-busboy": "4.4.0",
    "tsd": "0.13.1",
    "web-streams-polyfill": "2.1.1"
  },
  "dependencies": {
    "@babel/runtime": "7.10.2",
    "mime-types": "2.1.27"
  }
